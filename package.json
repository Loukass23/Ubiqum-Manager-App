    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "concurrently": "^5.0.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0"
  }
